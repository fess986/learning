{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,  // правило проверки соответствия энтера - юниксу
        "no-multiple-empty-lines": 0, // отключаем правило по многим энтерам
        "no-unused-vars": 0
    }
}
